<robot name="PRP">    

    <link name = "base_link">
        <visual>
            <geometry>
                <cylinder length="0.4" radius="0.025" />
            </geometry> 
            <material name = "base">
                <color rgba="0 1 0 1" />
            </material>
            <origin xyz="0 0 0.2" />
        </visual>
        <visual>
            <origin xyz="0 0 0.4" rpy="1.57 0 0" />
            <geometry>
                <cylinder length="0.06" radius="0.03" />
            </geometry>
            <material name="rot">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" /> <!-- center of mass -->
            <mass value="0" />
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>

    <link name = "Link2">
        <visual>
            <origin xyz="0.2 0 0" rpy= "0 1.57 0" />
            <geometry>
                <cylinder length="0.4" radius="0.02"/>
            </geometry> 
            <material name = "rot">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
        <visual>
            <origin xyz="0.4 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder length="0.05" radius="0.035" />
            </geometry>
            <material name="rot">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" /> <!-- center of mass -->
            <mass value="0" />
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>

    <link name = "Link3">
        <visual>
            <origin xyz="0.15 0 0" rpy="0 1.57 0" />
            <geometry>
                <cylinder length="0.3" radius="0.015" />
            </geometry> 
            <material name = "rot">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
        <visual>
            <origin xyz="0.3 0 0" rpy="0 0 0" />
            <geometry>
                <box size="0.04 0.04 0.06" />
            </geometry>
            <material name="prism">
                <color rgba="0 0 1 1" />
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0" />
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>

    <link name = "Link4">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <box size="0.025 0.025 0.4"/>
            </geometry> 
            <material name = "prism">
                <color rgba="0 0.0 1 1" />
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0" />
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>

    <link name="ee" />

    <joint name = "Joint1" type="revolute">
        <parent link = "base_link" />
        <child link = "Link2" />
        <origin xyz = "0 0 0.4" rpy="1.57 0 0"/>
        <axis xyz = "0 0 1" />
        <limit lower = "-1.2" upper = "+4.4" effort = "0" velocity = "1" /> 
        <dynamics damping="0.0" friction="0.0"/>
    </joint>


    <joint name = "Joint2" type="revolute">
        <parent link = "Link2" />
        <child link = "Link3" />
        <origin xyz = "0.4 0 0" rpy="0 0 0"/>
        <axis xyz = "0 0 1" />
        <limit lower = "-2.0" upper = "+2.0" effort = "0" velocity = "1"/>
        <dynamics damping="0.0" friction="0.0"/>
    </joint>


    <joint name = "Joint3" type="prismatic">
        <parent link = "Link3" />
        <child link = "Link4" />
        <origin xyz = "0.3 0 0.2" rpy = "0 0 0"/>
        <axis xyz = "0 0 1" />
        <limit lower = "-0.2" upper = "0" effort = "0" velocity = "1" />
        <dynamics damping="0.0" friction="0.0"/>
    </joint>

    <joint name="ee_link" type="fixed">
    	<parent link="Link4" />
    	<child link="ee" />
        <origin xyz="0 0 0.2" rpy="0 0 0" />
    </joint>
</robot>
